generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Inspection {
  id          Int      @id @default(autoincrement())
  equipmentId String
  inspector   String
  status      Status
  notes       String?
  createdAt   DateTime @default(now())
}

enum Status {
  OK
  NOK
  CRITICAL
}
